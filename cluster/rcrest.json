{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "name": "restrc",
    "labels": {
      "env": "dev"
    }
  },
  "spec": {
    "replicas": 6,
    "template": {
      "metadata": {
      },
      "spec": {
        "volumes": null,
        "containers": [
          {
            "name": "rest",
            "image": "quay.io/ywarezk/rest"
          },
          {
            "name": "nginx",
            "image": "quay.io/ywarezk/restnginx"
          }
        ]
      }
    }
  }
}
